{
  "name": "projeto-vetclinic-",
  "version": "1.0.0",
  "description": "Projeto desenvolvido durante a trilha de Node.js - AWS na Compass Uol.",
  "main": "app.js",
  "scripts": {
    "build": "npx tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node ./build/src/shared/infra/http/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "lint": "eslint ./src  --ext .ts",
    "lint-fix": "eslint ./src ./tests --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" ",
    "style:fix": "prettier --write \"src/**/*.ts\" ",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- migration:create  src/shared/infra/typeorm/migrations/",
    "migration:run": "npm run typeorm -- migration:run -d src/shared/infra/typeorm/dataSource.ts",
    "test:cov": "vitest run --coverage",
    "test:integration": "vitest run --dir ./tests/integration",
    "test:e2e": "vitest run --dir ./tests/e2e",
    "test:mutation": "npx stryker run",
    "prepare": "husky"
  },
  "keywords": [
    "Cl√≠nica",
    "NodeJS",
    "Sequelize",
    "Pets"
  ],
  "author": "Emanuel Ernesto de Andrade Rego",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "i": "^0.3.7",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "npm": "^10.9.0",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "projeto-vetclinic-": "file:",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
